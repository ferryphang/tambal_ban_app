<%= form_for(@category, url: categories_path, html: {multipart: true}) do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :picture_marker %>
    <%= f.file_field :picture_marker %>
  </div>
  <div class="field">
    <%= f.submit %>
  </div>
<% end %>